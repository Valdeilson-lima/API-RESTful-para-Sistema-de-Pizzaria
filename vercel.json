{
    "version": 2,
    "name": "api-pizzaria-backend-valdeilson",
    "builds":[
        {
            "src": "dist/server.js",
            "use": "@vercel/node",
            "config": {"includeFiles": ["dist/**"]}
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dist/server.js"
        }
    ]

    
}