{
    "version": "1.0.0",
    "builds":[
        {
            "src": "dist/server.js",
            "use": "@vercel/node",
            "config": {"includeFiles": ["dist/**"]}
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dist/server.js"
        }
    ]

    
}